{
  "static": {
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|js|css)",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=31536000, s-maxage=31536000"
        }]
      },
      {
        "source": "index.html",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=0, s-maxage=0, no-cache"
        }]
      }
    ],
    "public": "dist"
  }
}
